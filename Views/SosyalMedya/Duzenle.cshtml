@model MvcCv.Models.Entity.TblSosyalMedyalarim
@{
    ViewBag.Title = "Duzenle";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}
<h2>Sosyal Medya Düzenleme Sayfası</h2>
<br />
<form method="post">
    @Html.Label("Medya Adı")
    @Html.TextBoxFor(x => x.Ad, new { @class = "form-control" })
    <br />
    @Html.Label("Medya Linki")
    @Html.TextBoxFor(x => x.Link, new { @class = "form-control" })
    <br />
    @Html.Label("Medya İkonu")
    @Html.TextBoxFor(x => x.ikon, new { @class = "form-control" })
    <br />
    @Html.Label("Durum")
    @Html.TextBoxFor(x => x.Durum, new { @class = "form-control" })
    <br />
    <button class="btn btn-success">Güncelle</button>
</form>

