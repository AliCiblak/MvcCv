
@{
    ViewBag.Title = "DeneyimEkle";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}
<br />
<h4>Yeni Deneyim Ekle</h4>
<br />
<form class="form-group" method="post">
    <input type="text" name="Baslik" class="form-control" placeholder="Başlığı yazınız" />
    <br />
    <input type="text" name="Altbaslik" class="form-control" placeholder="Altbaşlığı yazınız" />
    <br />
    <input type="text" name="Tarih" class="form-control" placeholder="Tarihi yazınız" />
    <br />
    <textarea placeholder="Açıklamayı yazınız" name="Aciklama" class="form-control" rows="5"></textarea>
    <br />    
    <button class="btn btn-info">Kaydet</button>
</form>